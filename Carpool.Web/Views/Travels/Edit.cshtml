@model Carpool.Web.GeneratedModels.Travel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Travel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TravelId" />
            <div class="form-group">
                <label asp-for="TravelDriverId" class="control-label"></label>
                <select asp-for="TravelDriverId" class="form-control" asp-items="ViewBag.TravelDriverId"></select>
                <span asp-validation-for="TravelDriverId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelOrigin" class="control-label"></label>
                <input asp-for="TravelOrigin" class="form-control" />
                <span asp-validation-for="TravelOrigin" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelDestination" class="control-label"></label>
                <input asp-for="TravelDestination" class="form-control" />
                <span asp-validation-for="TravelDestination" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelDepartureTime" class="control-label"></label>
                <input asp-for="TravelDepartureTime" class="form-control" />
                <span asp-validation-for="TravelDepartureTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelAvailable" class="control-label"></label>
                <input asp-for="TravelAvailable" class="form-control" />
                <span asp-validation-for="TravelAvailable" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TravelMap" class="control-label"></label>
                <input asp-for="TravelMap" class="form-control" />
                <span asp-validation-for="TravelMap" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
